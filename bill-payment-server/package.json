{
  "name": "bill-payment-server",
  "version": "1.0.0",
  "description": "Mobile bill payments server",
  "keywords": [
    "loopback-application",
    "loopback"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9"
  },
  "scripts": {
    "build": "lb-tsc",
    "build:watch": "lb-tsc --watch",
    "clean": "lb-clean dist *.tsbuildinfo",
    "lint": "npm run prettier:check && npm run eslint",
    "lint:fix": "npm run eslint:fix && npm run prettier:fix",
    "prettier:cli": "lb-prettier \"**/*.ts\" \"**/*.js\"",
    "prettier:check": "npm run prettier:cli -- -l",
    "prettier:fix": "npm run prettier:cli -- --write",
    "eslint": "lb-eslint --report-unused-disable-directives .",
    "eslint:fix": "npm run eslint -- --fix",
    "pretest": "npm run clean && npm run build",
    "posttest": "npm run lint",
    "migrate": "node ./dist/migrate",
    "prestart": "npm run build",
    "start": "pm2 start ecosystem.config.js --watch --time --env production",
    "stop": "pm2 stop ecosystem.config.js --env production",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git"
  },
  "author": "",
  "license": "",
  "files": [
    "README.md",
    "index.js",
    "index.d.ts",
    "dist",
    "src",
    "!*/__tests__"
  ],
  "dependencies": {
    "@loopback/boot": "^2.0.1",
    "@loopback/context": "^3.1.0",
    "@loopback/core": "^2.1.0",
    "@loopback/openapi-v3": "^3.1.0",
    "@loopback/repository": "^2.0.1",
    "@loopback/rest": "^3.3.2",
    "@loopback/rest-explorer": "^2.0.1",
    "@loopback/service-proxy": "^2.0.1",
    "loopback-connector-mysql": "^5.4.3",
    "pm2": "^4.2.3"
  },
  "devDependencies": {
    "@loopback/build": "^9.0.6",
    "@loopback/eslint-config": "^6.0.1",
    "@loopback/testlab": "^2.0.1",
    "@types/node": "^10.17.17",
    "@typescript-eslint/eslint-plugin": "^2.24.0",
    "@typescript-eslint/parser": "^2.24.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-eslint-plugin": "^2.2.1",
    "eslint-plugin-mocha": "^6.3.0",
    "source-map-support": "^0.5.16",
    "typescript": "^5.0.4"
  }
}
