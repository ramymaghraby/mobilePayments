{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "paths": {
      "@app/*": [
        "app/*"
      ],
      "@config/*": [
        "app/_config/*"
      ],
      "@environment/*": [
        "environments/*"
      ],
      "@shared/*": [
        "app/_shared/*"
      ],
      "@helpers/*": [
        "helpers/*"
      ],
      "@services/*": [
        "src/app/services/*"
      ],
      "@models/*": [
        "src/app/models/*"
      ],
      "@masterData/*": [
        "src/app/pages/master-data/*"
      ],
      "@pages/*": [
        "src/app/pages/*"
      ],
      "jszip": [
        "node_modules/jszip/dist/jszip.min.js"
      ]
    },
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "es2020",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es5",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ]
  }
}
